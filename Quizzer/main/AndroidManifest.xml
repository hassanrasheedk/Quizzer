<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hassa.testapp2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatingQuizActivity"
            android:label="@string/title_activity_creating_quiz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddingQuestionsActivity"
            android:label="@string/title_activity_adding_questions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AfterQuizCreation"
            android:label="@string/title_activity_after_quiz_creation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentActivity"
            android:label="@string/title_activity_student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AttemptingQuiz"
            android:label="@string/title_activity_attempting_quiz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QuizFinished"
            android:label="@string/title_activity_quiz_finished"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>